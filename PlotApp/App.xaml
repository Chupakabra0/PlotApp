<Application x:Class="PlotApp.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             StartupUri="MVVM/Views/PlotView/PlotView.xaml">

    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Resources/Styles/Colors.xaml"></ResourceDictionary>
                <ResourceDictionary Source="Resources/Styles/Fonts.xaml"></ResourceDictionary>
                <ResourceDictionary Source="Resources/Styles/Style.xaml"></ResourceDictionary>
                <ResourceDictionary Source="Resources/Styles/Converters.xaml"></ResourceDictionary>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>

</Application>